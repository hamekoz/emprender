<h1>Listando domicilios</h1>

<table>
  <tr>
    <th>Calle</th>
    <th>Numero</th>
    <th>Piso</th>
    <th>Departamento</th>
    <th>Observacion</th>
    <th>Barrio</th>
    <th colspan="3" rowspan="1">
      <%= boton_agregar new_domicilio_path %>
    </th>
  </tr>

<% @domicilios.each do |domicilio| %>
  <tr>
    <td><%= domicilio.calle %></td>
    <td><%= domicilio.numero %></td>
    <td><%= domicilio.piso %></td>
    <td><%= domicilio.departamento %></td>
    <td><%= domicilio.observacion %></td>
    <td><%= domicilio.domicilio %></td>
    <td><%= boton_mostrar domicilio %></td>
    <td><%= boton_editar edit_domicilio_path(domicilio) %></td>
    <td><%= boton_eliminar domicilio %></td>
  </tr>
<% end %>
</table>

<br />
