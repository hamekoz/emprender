<h1>Listing eventos</h1>

<table>
  <tr>
    <th>Titulo</th>
    <th>Fechadeinicio</th>
    <th>Fechadefinalizacion</th>
    <th>Lugar</th>
    <th>Descripcion</th>
    <th>Publicado</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @eventos.each do |evento| %>
  <tr>
    <td><%= evento.titulo %></td>
    <td><%= evento.fechaDeInicio %></td>
    <td><%= evento.fechaDeFinalizacion %></td>
    <td><%= evento.lugar %></td>
    <td><%= evento.descripcion %></td>
    <td><%= evento.publicado %></td>
    <td><%= link_to 'Show', evento %></td>
    <td><%= link_to 'Edit', edit_evento_path(evento) %></td>
    <td><%= link_to 'Destroy', evento, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Evento', new_evento_path %>
