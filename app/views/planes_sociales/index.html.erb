<h1>Listado de planes sociales</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Descripcion</th>
    <th>Institucion</th>
    <th colspan="3" rowspan="1">
      <%= boton_agregar new_plan_social_path %>
    </th>
  </tr>

<% @planes_sociales.each do |plan_social| %>
  <tr>
    <td><%= plan_social.nombre %></td>
    <td><%= plan_social.descripcion %></td>
    <td><%= plan_social.institucion_id %></td>
    <td><%= boton_mostrar plan_social %></td>
    <td><%= boton_editar edit_plan_social_path(plan_social) %></td>
    <td><%= boton_eliminar plan_social %></td>
  </tr>
<% end %>
</table>
